<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="main.java.action.HelpAction"
            prefHeight="400.0" prefWidth="600.0">

    <Label fx:id="helpHeader" layoutX="200" layoutY="50" text="Help" textFill="brown">
        <font>
            <Font size="20" />
        </font>
    </Label>
    <Text id="helpText" layoutY="100" layoutX="20" fx:id="helpString" />
    <Button fx:id="back" layoutX="400" layoutY="350" text="Back" mnemonicParsing="false" />

</AnchorPane>
