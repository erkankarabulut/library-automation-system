<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="main.java.action.ChangePasswordAction"
            prefHeight="400.0" prefWidth="600.0">

    <Label layoutX="180.0" layoutY="20.0" text="Change Password" textFill="brown" >
        <font>
            <Font size="20" />
        </font>
    </Label>
    <Label layoutX="30.0" layoutY="60.0" text="Username: " />
    <Label layoutX="30.0" layoutY="100.0" text="ID: " />
    <Label layoutX="30.0" layoutY="140.0" text="Password: " />
    <Label layoutX="30.0" layoutY="180.0" text="New Password: " />
    <Label fx:id="errorMessage" layoutX="30.0" layoutY="210.0" textFill="brown" />
    <TextField fx:id="usernameField" layoutX="200.0" layoutY="60.0" />
    <TextField fx:id="idField" layoutX="200.0" layoutY="100.0" />
    <PasswordField fx:id="pswField" layoutX="200.0" layoutY="140.0" />
    <PasswordField fx:id="pswField2" layoutX="200.0" layoutY="180.0" />
    <Button fx:id="changePsw" layoutX="200.0" layoutY="250.0" mnemonicParsing="false" text="Change" />
    <Button fx:id="loginPage" layoutX="320.0" layoutY="250.0" mnemonicParsing="false" text="Login" />


</AnchorPane>
